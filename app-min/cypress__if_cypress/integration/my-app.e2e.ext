/// <reference types="Cypress" />

context('app', () => {
  it('shows message', () => {
    cy.visit('/');
    // @if shadow-dom
    cy.shadowGet('app').shadowFind('div').shadowContains('Hello World!');
    // @endif
    // @if !shadow-dom
    cy.get('app>div').contains('Hello World!');
    // @endif
  });
});
