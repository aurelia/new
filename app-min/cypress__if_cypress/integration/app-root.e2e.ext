/// <reference types="Cypress" />

context('app-root', () => {
  it('shows message', () => {
    cy.visit('/');
    // @if shadow-dom
    cy.shadowGet('app-root').shadowFind('div').shadowContains('Hello World!');
    // @endif
    // @if !shadow-dom
    cy.get('app-root>div').contains('Hello World!');
    // @endif
  });
});
